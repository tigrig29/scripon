{
  "name": "scripon",
  "version": "1.0.0",
  "description": "My flawless Nuxt.js project",
  "author": "tigrig",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "yarn convmd-init & nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev-debug": "yarn convmd-init & node --inspect-brk=9229 node_modules/nuxt/bin/nuxt",
    "rm-content-json": "rmdir content\\json /s /q",
    "convmd": "processmd \"content/markdown/**/*.md\" --outputDir content/json --summaryOutput content/json/summary.json",
    "convmd-init": "yarn rm-content-json & yarn convmd"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/google-adsense": "^1.1.3",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/style-resources": "^0.1.2",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.24",
    "cross-env": "^5.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "nuxt": "^3.4.3",
    "nuxt-fontawesome": "^0.4.0",
    "vuedraggable": "^2.21.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^10.0.1",
